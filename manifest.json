{
  "manifest_version": 2,
  "name": "forp for Chrome",
  "version": "1.1.2",
  "description": "PHP live profiler. Chrome client for forp PHP profiler.",
  "icons": {"16": "icon16.png", "48": "icon48.png", "128": "icon128.png"},
  "background": {"scripts": ["background.js", "forp-ui/src/built/forp.min.js"]},
  "minimum_chrome_version": "10.0",
  "permissions": ["tabs","http://*/*","https://*/*"],
  "devtools_page": "devtools.html"
}